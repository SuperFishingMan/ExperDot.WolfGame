<UserControl x:Class="CellBoxEx"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WolfGameLib"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Opacity="0.6">
        <Line Stroke="Black" X1="20" X2="20" Y2="40"/>
        <Line Stroke="Black" X2="40" Y2="20" Y1="20"/>
        <Line Stroke="Black" X2="20" Y1="20"/>
        <Line Stroke="Black" X2="40" X1="20" Y2="20"/>
        <Line Stroke="Black" X2="40" Y1="40" Y2="20" X1="20"/>
        <Line Stroke="Black" X2="20" Y1="20" Y2="40"/>
    </Grid>
</UserControl>
