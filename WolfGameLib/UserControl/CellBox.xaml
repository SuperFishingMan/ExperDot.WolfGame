<UserControl x:Class="CellBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WolfGameLib"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Opacity="0.6">
        <Rectangle Stroke="Black" HorizontalAlignment="Left" VerticalAlignment="Top" Width="40" Height="40"></Rectangle>
        <Line x:Name="Line0" Y2="40" Stroke="Black" X2="40" />
        <Line Visibility="Collapsed" x:Name="Line1" Y2="40" Stroke="Black" X1="40" />
    </Grid>
</UserControl>
